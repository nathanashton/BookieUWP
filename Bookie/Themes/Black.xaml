    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">




    <Brush x:Key="NavBarBackground">Black</Brush>
    <Brush x:Key="NavBarForeground">White</Brush>

    <Brush x:Key="CommandBarBackground">Black</Brush>
    <Brush x:Key="CommandBarForeground">White</Brush>

    <Brush x:Key="MainBackground">#2b2b2b</Brush>
    <Brush x:Key="MainText">White</Brush>
    <Brush x:Key="SubText">#BCBCB6</Brush>

    <Brush x:Key="LettersBackground">Black</Brush>
    <Brush x:Key="LettersBackgroundHighlighted">White</Brush>
    <Brush x:Key="LettersForeground">White</Brush>
